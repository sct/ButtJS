{
  "name": "buttbot-discord",
  "version": "1.4.5",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "start": "node dist",
    "dev": "nodemon -e ts -x ts-node src/index.ts",
    "debug": "nodemon -e ts -x ts-node --inspect src/index.ts",
    "build": "rimraf dist && tsc",
    "test": "yarn run lint",
    "lint": "eslint \"src/**/*.{js,ts}\" --ignore-path .gitignore",
    "prettier": "prettier --write \"src/**/*.{js,ts}\""
  },
  "author": "sct",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "2.8.7",
    "@types/nedb": "1.8.10",
    "@types/pluralize": "0.0.29",
    "@types/valid-url": "1.0.3",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "babel-eslint": "10.1.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.2.5",
    "lint-staged": "10.2.13",
    "nodemon": "2.0.4",
    "prettier": "2.0.5",
    "ts-node": "9.0.0",
    "typescript": "4.0.2"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "discord.js": "12.3.1",
    "dotenv": "^8.2.0",
    "fastify": "^3.2.1",
    "fastify-cors": "^4.1.0",
    "hyphenation.en-us": "^0.2.1",
    "hypher": "^0.2.5",
    "nedb": "^1.8.0",
    "pluralize": "^8.0.0",
    "valid-url": "^1.0.9",
    "winston": "^3.3.3"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write"
    ]
  }
}
