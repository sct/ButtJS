{
  "name": "buttbot-discord",
  "version": "1.4.3",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "start": "node dist",
    "dev": "nodemon -e ts -x ts-node src/index.ts",
    "debug": "nodemon -e ts -x ts-node --inspect src/index.ts",
    "build": "rimraf dist && tsc",
    "test": "yarn run lint",
    "lint": "eslint \"src/**/*.{js,ts}\" --ignore-path .gitignore"
  },
  "author": "sct",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "2.8.6",
    "@types/nedb": "1.8.9",
    "@types/pluralize": "0.0.29",
    "@types/valid-url": "1.0.2",
    "@typescript-eslint/eslint-plugin": "2.31.0",
    "@typescript-eslint/parser": "2.20.0",
    "babel-eslint": "10.0.3",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "4.2.3",
    "lint-staged": "10.0.7",
    "nodemon": "2.0.2",
    "prettier": "1.19.1",
    "ts-node": "8.6.2",
    "typescript": "3.8.2"
  },
  "dependencies": {
    "@discordjs/uws": "^11.149.1",
    "axios": "^0.19.2",
    "cors": "^2.8.5",
    "discord.js": "^11.5.1",
    "dotenv": "^8.2.0",
    "fastify": "^2.12.0",
    "hyphenation.en-us": "^0.2.1",
    "hypher": "^0.2.5",
    "nedb": "^1.8.0",
    "pluralize": "^8.0.0",
    "valid-url": "^1.0.9",
    "winston": "^3.2.1"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write"
    ]
  }
}
